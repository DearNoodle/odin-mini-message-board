<h1><%= title %></h1>
<a href="/new">Add New Message</a>
<ul>
    <% messages.forEach(message => { %>
        <% const date = message.added;%>
        <li>
            <h2><%= message.user %></h2>
            <p><%= 'Message: '+ message.text %></p>
            <p><%= `Date: ${date.getDate()}/${date.getMonth()+1}/${date.getFullYear()}` %></p>
        </li>
    <% }) %>
</ul>